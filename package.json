{
  "name": "turtle-statistics",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "ESLINT_USE_FLAT_CONFIG=true npx eslint --fix './src/**/*.js' './src/**/*.jsx'",
    "prettier": "prettier --write ./src",
    "preview": "vite preview",
    "commit": "git add . && git-cz",
    "prepare": "husky"
  },
  "config": {
    "cz-customizable": {
      "config": "./config.cjs"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "dependencies": {
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "sass": "^1.80.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^7.2.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.14",
    "unplugin-auto-import": "^0.18.3",
    "vite": "^5.4.9",
    "vite-plugin-svg-icons": "^2.0.1"
  }
}
